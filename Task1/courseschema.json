{
    "$schema": "http://json-schema.org/draft-04/schema",
        "title": "Courses List",
        "description": "List of courses that can be taken at the University of Northampton",
        "type": "object",
        "properties": {
            "courses": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "id": {"type": "integer"},
                        "course": {"type": "string"},
                        "ucascode": {},
                        "level": {},
                        "starting": {},
                        "duration": {"type": "object",
                            "properties": {
                                "fulltime": {"type": "string"},
                                "parttime": {"type": "string"},
                                "fulltimefoundation": {"type": "string"}
                            }},
                        
                    }
                }
            }
        }
}
{
    "$schema": "http://json-schema.org/draft-04/schema",
        "title": "Courses List",
        "description": "List of courses that can be taken at the University of Northampton",
        "type": "object",
        "properties": {
            "courses": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "id": {"type": "integer"},
                        "course": {"type": "string"},
                        "ucascode": {"type": "string"},
                        "level": {"type": "string"},
                        "starting": {"type": "string"},
                        "duration": {"type": "object",
                            "properties": {
                                "fulltime": {"type": "string"},
                                "parttime": {"type": "string"},
                                "fulltimefoundation": {"type": "string"}
                            }},
                        "fees": {"type": "object",
                            "properties": {
                                "ukfees": {"type": "object",
                                    "properties": {
                                        "fulltime": {"type": "integer"},
                                        "parttime": {"type": "integer"},
                                        "integratedfoundationyear": {"type": "integer"}
                                    }},
                                "internationalfees": {"type": "object",
                                    "properties": {
                                        "fulltime": {"type": "integer"},
                                        "parttime": {"type": "integer"},
                                        "integratedfoundationyear": {"type": "integer"}
                                    }},
                                "optionalworkplacementyear": {"type": "integer"}
                            }},
                        "location": {"type": "string"},
                        "contact": {"type": "object",
                            "properties": {
                                "ukfees": {"type": "object",
                                    "properties": {
                                        "fulltime": {"type": "integer"},
                                        "parttime": {"type": "integer"},
                                        "integratedfoundationyear": {"type": "integer"}
                                    }},
                                "internationalfees": {"type": "object",
                                    "properties": {
                                        "fulltime": {"type": "integer"},
                                        "parttime": {"type": "integer"},
                                        "integratedfoundationyear": {"type": "integer"}
                                    }},
                                "optionalworkplacementyear": {"type": "integer"}
                            }},
                    }
                }
            }
        }
        
}